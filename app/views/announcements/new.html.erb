<%= form_for :annoucement, url: announcements_path do |f| %>
  <p>
    <%= f.label :announcer %><br>
    <%= f.text_field :announcer %>
  </p>
 
  <p>
    <%= f.label :title %><br>
    <%= f.text_area :title %>
  </p>
 
  <p>
    <%= f.label :detail %><br>
    <%= f.text_area :detail %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
